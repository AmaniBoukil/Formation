<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form_inherit_vehicle" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.vehicle</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="fleet.res_config_settings_view_form"/>
        <field name="arch" type="xml">

            <xpath expr="//div[@id='fleet']" position="inside">

                <h2>Default Analytic Plan</h2>

                <div class="row mt16 o_settings_container" id="hospital_setting">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane"/>
                        <div class="o_setting_right_pane">

                            <span class="o_form_label">Analytic Plan</span>

                            <div class="text-muted content-group mt16">
                                <field name="analytic_plan_vehicle" class="oe_inline" />
                            </div>

                        </div>
                    </div>
                </div>


            </xpath>

        </field>
    </record>
</odoo>
